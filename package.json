{
  "name": "realty-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "node scripts/export-code.js",
    "build": "next build --no-lint",
    "build:fast": "NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max_old_space_size=4096' SKIP_EXAMPLE_PAGES=true next build --no-lint",
    "build:prod": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production EXPORT_CODE=true npm run prebuild && next build --no-lint",
    "vercel-build": "prisma generate && next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "seed-listings": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed-listings.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@sentry/nextjs": "^9.14.0",
    "@tanstack/react-query": "^5.74.4",
    "@vercel/blob": "^1.0.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.9.1",
    "jose": "^5.2.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.488.0",
    "multer": "^2.0.0",
    "next": "15.3.0",
    "nodemailer": "^7.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-range": "^1.10.0",
    "sharp": "^0.34.1",
    "swr": "^2.3.3",
    "uuid": "^9.0.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^9.0.8",
    "archiver": "^7.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "prisma": "^6.8.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
